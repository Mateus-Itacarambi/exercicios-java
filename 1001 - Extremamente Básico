import java.io.IOException;
import java.util.Scanner;
 
public class Basico {
 
    public static void main(String[] args) throws IOException {
    	Scanner sc = new Scanner(System.in);
    	int A = sc.nextInt();
    	int B = sc.nextInt();
    	int X = A + B;
    	
    	System.out.printf("x = %d\n", X);
    }
 
}
